<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox alignment="center" prefHeight="400" prefWidth="600" spacing="5.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ifpr.pgua.eic.sistemaEscola.controllers.JanelaListarAlunosMatriculados">

      <VBox alignment="center" prefHeight="400.0" prefWidth="306.0" spacing="5.0">
        <ListView fx:id="cursos" onMouseClicked="#exibirAlunos" />

        <Button id="btVoltar" onAction="#carregaMenuAlunos" prefWidth="100" text="Voltar"/>
      </VBox>

      <ListView fx:id="alunos" />

</HBox>
